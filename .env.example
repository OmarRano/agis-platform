# Environment Variables for DigiAGIS Platform

# API Configuration (update for production)
REACT_APP_API_URL=<YOUR_API_URL>

# Firebase or Auth Config (if needed)
REACT_APP_AUTH_PROVIDER=mock

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PAYMENTS=true

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=

# Payment Gateway (Stripe, PayPal, etc.)
REACT_APP_STRIPE_PUBLIC_KEY=<STRIPE_PUBLISHABLE_KEY>

# Email Service
REACT_APP_EMAIL_SERVICE=mailgun
